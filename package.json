{
  "name": "front-framework",
  "version": "1.0.0",
  "main": "index.js",
  "bin": {
    "boy": "dist/index.js"
  },
  "scripts": {
    "start": "tsc --noEmit && nodemon",
    "build": "npx babel bin --out-dir dist --extensions '.ts' --copy-files --ignore bin/smart/templates",
    "lint": "tsc --noEmit && eslint --fix 'bin/**/*.{js,ts}'",
    "test": "jest --forceExit --detectOpenHandles",
    "prettier:write": "npx prettier --write **/*.{js,ts,json} && npx prettier --write *.{js,ts,json}"
  },
  "description": "typescript, react, vue, front framework, smart",
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
