  },
  "homepage": "https://github.com/09boy/front-framework.git",
  "engines": {
    "node": ">=12.18.0",
    "npm:": ">= 6.9.0"
  },
  "settings": {
    "jest": {
      "version": 26
    }
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules/*",
      "dist/*",
      "__tests__/*"
    ],
    "watch": [
      "bin"
    ],
    "ext": "ts, js, yml",
    "exec": "npm run build",
    "delay": 2500
  },
  "copymonden": "npx babel-node -x .ts -- ./bin/index.ts",
